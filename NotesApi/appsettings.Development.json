{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "a-string-secret-at-least-256-bits-long",
    "Issuer": "NotesApi",
    "Audience": "NotesApiUsers",
    "GracePeriodInMinutes" : 10
  },
  "Enc": {
    "Secret": "a-strong-secret-encryption-key-256-bit",
    "Salt": "a-nonce-or-mixer"
  },
  "Paging": {
    "PageSize" : 20
  },
  "ForwardedHeaders": {
    "ForwardedHeaders": "XForwardedFor,XForwardedHost,XForwardedProto",
    "ForwardLimit": 1,
    "RequireHeaderSymmetry": true,
    "KnownProxies": [ "::1" ],
    "KnownNetworks": [ "127.0.0.1/8" ],
    "AllowedHosts": [ "google.com" ]
  }
}