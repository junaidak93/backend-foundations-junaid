{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=notesdb;Username=postgres;Password=postgres"
  },
  "Jwt": {
    "Key": "super-secret-key-change-this",
    "Issuer": "NotesApi",
    "Audience": "NotesApiUsers",
    "GracePeriodInMinutes" : 10
  },
  "Enc": {
    "Secret": "a-strong-secret-encryption-key-256-bit",
    "Salt": "a-nonce-or-mixer"
  }
}